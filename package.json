  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "cp -r ./src/platform/node/files ./output/platform/node && tsc -watch -p ./",
    "dev": "node ./output/platform/node/http.js",
    "web": "cd ./src/platform/web && npm run serve",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.12",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "^16.18.3",
    "@types/pg": "^8.6.5",
    "axios": "^1.1.3",
    "dockerode": "^3.3.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "pg": "^8.8.0",
    "socket.io": "^4.5.4",
    "tar-fs": "^2.1.1"
  }
